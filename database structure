queryInterface.createTable('sentences', {
      id: {
        type: INTEGER,
        primaryKey: true,
        autoIncrement: true
      },//sentence Id
      pageNumber: INTEGER,
      date: STRING,//日期
      originSentences: STRING,
      english: STRING,
      created_at: DATE,
      updated_at: DATE,
    }, {
      charset: 'utf8'
    });
  },




queryInterface.createTable('languageReferences', {
      id: {
        type: INTEGER,
        primaryKey: true,
        autoIncrement: true
      },//sentence Id
      sentenceId: STRING,
      english: STRING,
      Belgium: STRING,
      France: STRING,
      created_at: DATE,
      updated_at: DATE,
    }, {
      charset: 'utf8'
    });
  },

